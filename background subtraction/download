      function MakeArray(n) {
              this.length = n
              for (var i = 1; i<=n; i++) {
                      this[i] = new Image()
                      }
              return this
      }
/* Here's the Mouseover function. When it's called below, it will display
 * the proper highlighed Over image, and turn on the associated picture 
 * that goes with it
*/
        function msover(num) {
           document.images[0].src =  picture[num].src 
        }
